input_marks = float(input("Enter Scored Marks: "))

if input_marks <25:
    print("You have Scored a F :(")
if input_marks >=25 and input_marks<45:
    print("You have scored an E")
if input_marks>=45 and input_marks<50:
    print("You have scored a D")
if input_marks>=50 and input_marks<60:
    print("You have scored a C")
if input_marks>=60 and input_marks<80:
    print("You have scored a B")
if input_marks>=60:
    print("You have scored an A :)")
